<?xml version="1.0"?>
<rdf:RDF xmlns="https://haulaah.github.io/CIRPO#"
     xml:base="https://haulaah.github.io/CIRPO"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:terms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="https://haulaah.github.io/CIRPO">
        <terms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-06-01</terms:created>
        <terms:creator>Haula Galadima, Cormac Doherty, Rob Brennan</terms:creator>
        <terms:description>An ontology for modeling cybersecurity incident response processes</terms:description>
        <terms:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
        <terms:title>Cyber Incident Response Process Ontology (CIRPO)</terms:title>
        <owl:versionInfo>1.0</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/dc/terms/created -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/created"/>
    


    <!-- http://purl.org/dc/terms/creator -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/creator"/>
    


    <!-- http://purl.org/dc/terms/description -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/description"/>
    


    <!-- http://purl.org/dc/terms/license -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/license"/>
    


    <!-- http://purl.org/dc/terms/title -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/title"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/2001/XMLSchema#date -->


    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/ns/prov#used -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#used">
        <owl:inverseOf rdf:resource="https://haulaah.github.io/CIRPO#wasUsedBy"/>
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#CourseOfAction"/>
        <rdfs:range rdf:resource="https://haulaah.github.io/CIRPO#SecurityAnalysis"/>
        <rdfs:comment>Indicates that a course of action used specific analysis</rdfs:comment>
        <rdfs:label>used</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/prov#wasAssociatedWith -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#wasAssociatedWith">
        <owl:inverseOf rdf:resource="https://haulaah.github.io/CIRPO#hadAssociatedEntity"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://haulaah.github.io/CIRPO#Analyst"/>
                    <rdf:Description rdf:about="https://haulaah.github.io/CIRPO#Artefact"/>
                    <rdf:Description rdf:about="https://haulaah.github.io/CIRPO#SecurityEvent"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://haulaah.github.io/CIRPO#Incident"/>
        <rdfs:comment>Indicates association with an incident</rdfs:comment>
        <rdfs:label>was associated with</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/prov#wasGeneratedBy -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#wasGeneratedBy">
        <owl:inverseOf rdf:resource="https://haulaah.github.io/CIRPO#generated"/>
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#SecurityAnalysis"/>
        <rdfs:range rdf:resource="https://haulaah.github.io/CIRPO#SecurityEvent"/>
        <rdfs:comment>Indicates the security event that generated this analysis</rdfs:comment>
        <rdfs:label>was generated by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/prov#wasPerformedBy -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#wasPerformedBy">
        <owl:inverseOf rdf:resource="https://haulaah.github.io/CIRPO#performed"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://haulaah.github.io/CIRPO#SecurityAnalysis"/>
                    <rdf:Description rdf:about="https://haulaah.github.io/CIRPO#SecurityEvent"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://haulaah.github.io/CIRPO#Analyst"/>
        <rdfs:comment>Indicates the analyst who performed this activity</rdfs:comment>
        <rdfs:label>was performed by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://haulaah.github.io/CIRPO#assignedToCoa -->


    <owl:ObjectProperty rdf:about="https://haulaah.github.io/CIRPO#assignedToCoa">
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#Analyst"/>
        <rdfs:range rdf:resource="https://haulaah.github.io/CIRPO#CourseOfAction"/>
        <rdfs:comment>Inverse of hasCoaAssignedAnalyst. Links an analyst to the courses of action they are assigned to execute.</rdfs:comment>
        <rdfs:label>assigned to COA</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://haulaah.github.io/CIRPO#confirmedIncident -->


    <owl:ObjectProperty rdf:about="https://haulaah.github.io/CIRPO#confirmedIncident">
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#Analyst"/>
        <rdfs:range rdf:resource="https://haulaah.github.io/CIRPO#Incident"/>
        <rdfs:comment>Inverse of hasConfirmationAnalyst. Links an analyst to the incidents they confirmed.</rdfs:comment>
        <rdfs:label>confirmed incident</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://haulaah.github.io/CIRPO#createdArtefact -->


    <owl:ObjectProperty rdf:about="https://haulaah.github.io/CIRPO#createdArtefact">
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#Analyst"/>
        <rdfs:range rdf:resource="https://haulaah.github.io/CIRPO#Artefact"/>
        <rdfs:comment>Inverse of hasArtefactCreationAnalyst. Links an analyst to the artefacts they created.</rdfs:comment>
        <rdfs:label>created artefact</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://haulaah.github.io/CIRPO#createdCoa -->


    <owl:ObjectProperty rdf:about="https://haulaah.github.io/CIRPO#createdCoa">
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#Analyst"/>
        <rdfs:range rdf:resource="https://haulaah.github.io/CIRPO#CourseOfAction"/>
        <rdfs:comment>Inverse of hasCoaCreationAnalyst. Links an analyst to the courses of action they created.</rdfs:comment>
        <rdfs:label>created COA</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://haulaah.github.io/CIRPO#generated -->


    <owl:ObjectProperty rdf:about="https://haulaah.github.io/CIRPO#generated">
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#SecurityEvent"/>
        <rdfs:range rdf:resource="https://haulaah.github.io/CIRPO#SecurityAnalysis"/>
        <rdfs:comment>Inverse of prov:wasGeneratedBy. Indicates a security event generated an analysis.</rdfs:comment>
        <rdfs:label>generated</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://haulaah.github.io/CIRPO#generatedChat -->


    <owl:ObjectProperty rdf:about="https://haulaah.github.io/CIRPO#generatedChat">
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#Analyst"/>
        <rdfs:range rdf:resource="https://haulaah.github.io/CIRPO#ResponderCommunication"/>
        <rdfs:comment>Inverse of hasChatAnalyst. Links an analyst to the chat they generated.</rdfs:comment>
        <rdfs:label>generated chat</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://haulaah.github.io/CIRPO#hadAssociatedEntity -->


    <owl:ObjectProperty rdf:about="https://haulaah.github.io/CIRPO#hadAssociatedEntity">
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#Incident"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://haulaah.github.io/CIRPO#Analyst"/>
                    <rdf:Description rdf:about="https://haulaah.github.io/CIRPO#Artefact"/>
                    <rdf:Description rdf:about="https://haulaah.github.io/CIRPO#SecurityEvent"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>Inverse of prov:wasAssociatedWith. Indicates an incident had an associated entity.</rdfs:comment>
        <rdfs:label>had associated entity</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://haulaah.github.io/CIRPO#hasAnalysis -->


    <owl:ObjectProperty rdf:about="https://haulaah.github.io/CIRPO#hasAnalysis">
        <owl:inverseOf rdf:resource="https://haulaah.github.io/CIRPO#isAnalysisOf"/>
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#Incident"/>
        <rdfs:range rdf:resource="https://haulaah.github.io/CIRPO#SecurityAnalysis"/>
        <rdfs:comment>Links an incident to the security analyses performed on it.</rdfs:comment>
        <rdfs:label>has analysis</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://haulaah.github.io/CIRPO#hasAnalysisPerformingAnalyst -->


    <owl:ObjectProperty rdf:about="https://haulaah.github.io/CIRPO#hasAnalysisPerformingAnalyst">
        <owl:inverseOf rdf:resource="https://haulaah.github.io/CIRPO#performedAnalysis"/>
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#SecurityAnalysis"/>
        <rdfs:range rdf:resource="https://haulaah.github.io/CIRPO#Analyst"/>
        <rdfs:comment>Links a security analysis to the analyst who performed it.</rdfs:comment>
        <rdfs:label>has analysis performing analyst</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://haulaah.github.io/CIRPO#hasArtefact -->


    <owl:ObjectProperty rdf:about="https://haulaah.github.io/CIRPO#hasArtefact">
        <owl:inverseOf rdf:resource="https://haulaah.github.io/CIRPO#isArtefactOf"/>
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#Incident"/>
        <rdfs:range rdf:resource="https://haulaah.github.io/CIRPO#Artefact"/>
        <rdfs:comment>Links an incident to related artefacts/observables collected during the investigation.</rdfs:comment>
        <rdfs:label>has artefact</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://haulaah.github.io/CIRPO#hasArtefactCreationAnalyst -->


    <owl:ObjectProperty rdf:about="https://haulaah.github.io/CIRPO#hasArtefactCreationAnalyst">
        <owl:inverseOf rdf:resource="https://haulaah.github.io/CIRPO#createdArtefact"/>
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#Artefact"/>
        <rdfs:range rdf:resource="https://haulaah.github.io/CIRPO#Analyst"/>
        <rdfs:comment>Links an artefact to the analyst who collected, created, or documented it.</rdfs:comment>
        <rdfs:label>has artefact creation analyst</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://haulaah.github.io/CIRPO#hasChatAnalyst -->


    <owl:ObjectProperty rdf:about="https://haulaah.github.io/CIRPO#hasChatAnalyst">
        <owl:inverseOf rdf:resource="https://haulaah.github.io/CIRPO#generatedChat"/>
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#ResponderCommunication"/>
        <rdfs:range rdf:resource="https://haulaah.github.io/CIRPO#Analyst"/>
        <rdfs:comment>Links a communication to the analyst who made the chat.</rdfs:comment>
        <rdfs:label>has chat analyst</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://haulaah.github.io/CIRPO#hasCoa -->


    <owl:ObjectProperty rdf:about="https://haulaah.github.io/CIRPO#hasCoa">
        <owl:inverseOf rdf:resource="https://haulaah.github.io/CIRPO#isCoaOf"/>
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#Incident"/>
        <rdfs:range rdf:resource="https://haulaah.github.io/CIRPO#CourseOfAction"/>
        <rdfs:comment>Links an incident to the response actions (COA) taken to resolve it.</rdfs:comment>
        <rdfs:label>has course of action</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://haulaah.github.io/CIRPO#hasCoaAssignedAnalyst -->


    <owl:ObjectProperty rdf:about="https://haulaah.github.io/CIRPO#hasCoaAssignedAnalyst">
        <owl:inverseOf rdf:resource="https://haulaah.github.io/CIRPO#assignedToCoa"/>
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#CourseOfAction"/>
        <rdfs:range rdf:resource="https://haulaah.github.io/CIRPO#Analyst"/>
        <rdfs:comment>Links a course of action to the analyst responsible for executing it.</rdfs:comment>
        <rdfs:label>has COA assigned analyst</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://haulaah.github.io/CIRPO#hasCoaCreationAnalyst -->


    <owl:ObjectProperty rdf:about="https://haulaah.github.io/CIRPO#hasCoaCreationAnalyst">
        <owl:inverseOf rdf:resource="https://haulaah.github.io/CIRPO#createdCoa"/>
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#CourseOfAction"/>
        <rdfs:range rdf:resource="https://haulaah.github.io/CIRPO#Analyst"/>
        <rdfs:comment>Links a course of action to the analyst who created or planned it.</rdfs:comment>
        <rdfs:label>has COA creation analyst</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://haulaah.github.io/CIRPO#hasConfirmationAnalyst -->


    <owl:ObjectProperty rdf:about="https://haulaah.github.io/CIRPO#hasConfirmationAnalyst">
        <owl:inverseOf rdf:resource="https://haulaah.github.io/CIRPO#confirmedIncident"/>
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#Incident"/>
        <rdfs:range rdf:resource="https://haulaah.github.io/CIRPO#Analyst"/>
        <rdfs:comment>Links an incident to the analyst who confirmed it as a genuine security incident.</rdfs:comment>
        <rdfs:label>has confirmation analyst</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://haulaah.github.io/CIRPO#hasEvent -->


    <owl:ObjectProperty rdf:about="https://haulaah.github.io/CIRPO#hasEvent">
        <owl:inverseOf rdf:resource="https://haulaah.github.io/CIRPO#isEventOf"/>
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#Incident"/>
        <rdfs:range rdf:resource="https://haulaah.github.io/CIRPO#SecurityEvent"/>
        <rdfs:comment>Links an incident to one or more related security events that triggered or are part of it.</rdfs:comment>
        <rdfs:label>has event</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://haulaah.github.io/CIRPO#hasPertainingChat -->


    <owl:ObjectProperty rdf:about="https://haulaah.github.io/CIRPO#hasPertainingChat">
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#Incident"/>
        <rdfs:range rdf:resource="https://haulaah.github.io/CIRPO#ResponderCommunication"/>
        <rdfs:comment>Inverse of pertainsToIncident. Links an incident to a chat about it.</rdfs:comment>
        <rdfs:label>has pertaining chat</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://haulaah.github.io/CIRPO#isAnalysisOf -->


    <owl:ObjectProperty rdf:about="https://haulaah.github.io/CIRPO#isAnalysisOf">
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#SecurityAnalysis"/>
        <rdfs:range rdf:resource="https://haulaah.github.io/CIRPO#Incident"/>
        <rdfs:comment>Inverse of hasAnalysis. Links an analysis back to its incident.</rdfs:comment>
        <rdfs:label>is analysis of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://haulaah.github.io/CIRPO#isArtefactOf -->


    <owl:ObjectProperty rdf:about="https://haulaah.github.io/CIRPO#isArtefactOf">
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#Artefact"/>
        <rdfs:range rdf:resource="https://haulaah.github.io/CIRPO#Incident"/>
        <rdfs:comment>Inverse of hasArtefact. Links an artefact back to its incident.</rdfs:comment>
        <rdfs:label>is artefact of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://haulaah.github.io/CIRPO#isCoaOf -->


    <owl:ObjectProperty rdf:about="https://haulaah.github.io/CIRPO#isCoaOf">
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#CourseOfAction"/>
        <rdfs:range rdf:resource="https://haulaah.github.io/CIRPO#Incident"/>
        <rdfs:comment>Inverse of hasCoa. Links a course of action back to its incident.</rdfs:comment>
        <rdfs:label>is COA of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://haulaah.github.io/CIRPO#isEventOf -->


    <owl:ObjectProperty rdf:about="https://haulaah.github.io/CIRPO#isEventOf">
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#SecurityEvent"/>
        <rdfs:range rdf:resource="https://haulaah.github.io/CIRPO#Incident"/>
        <rdfs:comment>Inverse of hasEvent. Links a security event back to its parent incident.</rdfs:comment>
        <rdfs:label>is event of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://haulaah.github.io/CIRPO#performed -->


    <owl:ObjectProperty rdf:about="https://haulaah.github.io/CIRPO#performed">
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#Analyst"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://haulaah.github.io/CIRPO#SecurityAnalysis"/>
                    <rdf:Description rdf:about="https://haulaah.github.io/CIRPO#SecurityEvent"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>Inverse of prov:wasPerformedBy. Indicates an analyst performed an activity.</rdfs:comment>
        <rdfs:label>performed</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://haulaah.github.io/CIRPO#performedAnalysis -->


    <owl:ObjectProperty rdf:about="https://haulaah.github.io/CIRPO#performedAnalysis">
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#Analyst"/>
        <rdfs:range rdf:resource="https://haulaah.github.io/CIRPO#SecurityAnalysis"/>
        <rdfs:comment>Inverse of hasAnalysisPerformingAnalyst. Links an analyst to the analyses they performed.</rdfs:comment>
        <rdfs:label>performed analysis</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://haulaah.github.io/CIRPO#pertainsToIncident -->


    <owl:ObjectProperty rdf:about="https://haulaah.github.io/CIRPO#pertainsToIncident">
        <owl:inverseOf rdf:resource="https://haulaah.github.io/CIRPO#hasPertainingChat"/>
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#ResponderCommunication"/>
        <rdfs:range rdf:resource="https://haulaah.github.io/CIRPO#Incident"/>
        <rdfs:comment>Links a chat to the incident it discusses.</rdfs:comment>
        <rdfs:label>pertains to incident</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://haulaah.github.io/CIRPO#wasUsedBy -->


    <owl:ObjectProperty rdf:about="https://haulaah.github.io/CIRPO#wasUsedBy">
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#SecurityAnalysis"/>
        <rdfs:range rdf:resource="https://haulaah.github.io/CIRPO#CourseOfAction"/>
        <rdfs:comment>Inverse of prov:used. Indicates an analysis was used by a course of action.</rdfs:comment>
        <rdfs:label>was used by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://haulaah.github.io/CIRPO#hasAnalystEmail -->


    <owl:DatatypeProperty rdf:about="https://haulaah.github.io/CIRPO#hasAnalystEmail">
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#Analyst"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Email address of the analyst.</rdfs:comment>
        <rdfs:label>has analyst email</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://haulaah.github.io/CIRPO#hasAnalystId -->


    <owl:DatatypeProperty rdf:about="https://haulaah.github.io/CIRPO#hasAnalystId">
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#Analyst"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Unique identifier for an analyst.</rdfs:comment>
        <rdfs:label>has analyst ID</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://haulaah.github.io/CIRPO#hasArtefactCreationDateTime -->


    <owl:DatatypeProperty rdf:about="https://haulaah.github.io/CIRPO#hasArtefactCreationDateTime">
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#Artefact"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Timestamp when the artefact was collected.</rdfs:comment>
        <rdfs:label>has artefact creation datetime</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://haulaah.github.io/CIRPO#hasArtefactDataValue -->


    <owl:DatatypeProperty rdf:about="https://haulaah.github.io/CIRPO#hasArtefactDataValue">
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#Artefact"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The actual content or value of the artefact .</rdfs:comment>
        <rdfs:label>has artefact data value</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://haulaah.github.io/CIRPO#hasArtefactType -->


    <owl:DatatypeProperty rdf:about="https://haulaah.github.io/CIRPO#hasArtefactType">
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#Artefact"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Classification of the artefact.</rdfs:comment>
        <rdfs:label>has artefact type</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://haulaah.github.io/CIRPO#hasChatMessage -->


    <owl:DatatypeProperty rdf:about="https://haulaah.github.io/CIRPO#hasChatMessage">
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#ResponderCommunication"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The content or body of the communication message.</rdfs:comment>
        <rdfs:label>has chat message</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://haulaah.github.io/CIRPO#hasChatMessageDateTime -->


    <owl:DatatypeProperty rdf:about="https://haulaah.github.io/CIRPO#hasChatMessageDateTime">
        <rdfs:domain rdf:resource="https://haulaah.github.io/CIRPO#ResponderCommunication"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Timestamp when the communication occurred.</rdfs:comment>
        <rdfs:label>has chat message datetime</rdfs:label>
    </owl:DatatypeProperty>
    


