@prefix : <https://haulaah.github.io/CIRPO#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://haulaah.github.io/CIRPO#> .

<https://haulaah.github.io/CIRPO> rdf:type owl:Ontology ;
                                   <http://purl.org/dc/terms/created> "2025-06-01"^^xsd:date ;
                                   <http://purl.org/dc/terms/creator> "Haula Galadima, Cormac Doherty, Rob Brennan" ;
                                   <http://purl.org/dc/terms/description> "An ontology for modeling cybersecurity incident response processes" ;
                                   <http://purl.org/dc/terms/license> <https://creativecommons.org/licenses/by/4.0/> ;
                                   <http://purl.org/dc/terms/title> "Cyber Incident Response Process Ontology (CIRPO)" ;
                                   owl:versionInfo "1.0" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/created
<http://purl.org/dc/terms/created> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
<http://purl.org/dc/terms/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
<http://purl.org/dc/terms/license> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
<http://purl.org/dc/terms/title> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.w3.org/ns/prov#used
<http://www.w3.org/ns/prov#used> rdf:type owl:ObjectProperty ;
                                 owl:inverseOf :wasUsedBy ;
                                 rdfs:domain :CourseOfAction ;
                                 rdfs:range :SecurityAnalysis ;
                                 rdfs:comment "Indicates that a course of action used specific analysis" ;
                                 rdfs:label "used" .


###  http://www.w3.org/ns/prov#wasAssociatedWith
<http://www.w3.org/ns/prov#wasAssociatedWith> rdf:type owl:ObjectProperty ;
                                              owl:inverseOf :hadAssociatedEntity ;
                                              rdfs:domain [ rdf:type owl:Class ;
                                                            owl:unionOf ( :Analyst
                                                                          :Artefact
                                                                          :SecurityEvent
                                                                        )
                                                          ] ;
                                              rdfs:range :Incident ;
                                              rdfs:comment "Indicates association with an incident" ;
                                              rdfs:label "was associated with" .


###  http://www.w3.org/ns/prov#wasGeneratedBy
<http://www.w3.org/ns/prov#wasGeneratedBy> rdf:type owl:ObjectProperty ;
                                           owl:inverseOf :generated ;
                                           rdfs:domain :SecurityAnalysis ;
                                           rdfs:range :SecurityEvent ;
                                           rdfs:comment "Indicates the security event that generated this analysis" ;
                                           rdfs:label "was generated by" .


###  http://www.w3.org/ns/prov#wasPerformedBy
<http://www.w3.org/ns/prov#wasPerformedBy> rdf:type owl:ObjectProperty ;
                                           owl:inverseOf :performed ;
                                           rdfs:domain [ rdf:type owl:Class ;
                                                         owl:unionOf ( :SecurityAnalysis
                                                                       :SecurityEvent
                                                                     )
                                                       ] ;
                                           rdfs:range :Analyst ;
                                           rdfs:comment "Indicates the analyst who performed this activity" ;
                                           rdfs:label "was performed by" .


###  https://haulaah.github.io/CIRPO#assignedToCoa
:assignedToCoa rdf:type owl:ObjectProperty ;
               rdfs:domain :Analyst ;
               rdfs:range :CourseOfAction ;
               rdfs:comment "Inverse of hasCoaAssignedAnalyst. Links an analyst to the courses of action they are assigned to execute." ;
               rdfs:label "assigned to COA" .


###  https://haulaah.github.io/CIRPO#confirmedIncident
:confirmedIncident rdf:type owl:ObjectProperty ;
                   rdfs:domain :Analyst ;
                   rdfs:range :Incident ;
                   rdfs:comment "Inverse of hasConfirmationAnalyst. Links an analyst to the incidents they confirmed." ;
                   rdfs:label "confirmed incident" .


###  https://haulaah.github.io/CIRPO#createdArtefact
:createdArtefact rdf:type owl:ObjectProperty ;
                 rdfs:domain :Analyst ;
                 rdfs:range :Artefact ;
                 rdfs:comment "Inverse of hasArtefactCreationAnalyst. Links an analyst to the artefacts they created." ;
                 rdfs:label "created artefact" .


###  https://haulaah.github.io/CIRPO#createdCoa
:createdCoa rdf:type owl:ObjectProperty ;
            rdfs:domain :Analyst ;
            rdfs:range :CourseOfAction ;
            rdfs:comment "Inverse of hasCoaCreationAnalyst. Links an analyst to the courses of action they created." ;
            rdfs:label "created COA" .


###  https://haulaah.github.io/CIRPO#generated
:generated rdf:type owl:ObjectProperty ;
           rdfs:domain :SecurityEvent ;
           rdfs:range :SecurityAnalysis ;
           rdfs:comment "Inverse of prov:wasGeneratedBy. Indicates a security event generated an analysis." ;
           rdfs:label "generated" .


###  https://haulaah.github.io/CIRPO#generatedChat
:generatedChat rdf:type owl:ObjectProperty ;
               rdfs:domain :Analyst ;
               rdfs:range :ResponderCommunication ;
               rdfs:comment "Inverse of hasChatAnalyst. Links an analyst to the chat they generated." ;
               rdfs:label "generated chat" .


###  https://haulaah.github.io/CIRPO#hadAssociatedEntity
:hadAssociatedEntity rdf:type owl:ObjectProperty ;
                     rdfs:domain :Incident ;
                     rdfs:range [ rdf:type owl:Class ;
                                  owl:unionOf ( :Analyst
                                                :Artefact
                                                :SecurityEvent
                                              )
                                ] ;
                     rdfs:comment "Inverse of prov:wasAssociatedWith. Indicates an incident had an associated entity." ;
                     rdfs:label "had associated entity" .


###  https://haulaah.github.io/CIRPO#hasAnalysis
:hasAnalysis rdf:type owl:ObjectProperty ;
             owl:inverseOf :isAnalysisOf ;
             rdfs:domain :Incident ;
             rdfs:range :SecurityAnalysis ;
             rdfs:comment "Links an incident to the security analyses performed on it." ;
             rdfs:label "has analysis" .


###  https://haulaah.github.io/CIRPO#hasAnalysisPerformingAnalyst
:hasAnalysisPerformingAnalyst rdf:type owl:ObjectProperty ;
                              owl:inverseOf :performedAnalysis ;
                              rdfs:domain :SecurityAnalysis ;
                              rdfs:range :Analyst ;
                              rdfs:comment "Links a security analysis to the analyst who performed it." ;
                              rdfs:label "has analysis performing analyst" .


###  https://haulaah.github.io/CIRPO#hasArtefact
:hasArtefact rdf:type owl:ObjectProperty ;
             owl:inverseOf :isArtefactOf ;
             rdfs:domain :Incident ;
             rdfs:range :Artefact ;
             rdfs:comment "Links an incident to related artefacts/observables collected during the investigation." ;
             rdfs:label "has artefact" .


###  https://haulaah.github.io/CIRPO#hasArtefactCreationAnalyst
:hasArtefactCreationAnalyst rdf:type owl:ObjectProperty ;
                            owl:inverseOf :createdArtefact ;
                            rdfs:domain :Artefact ;
                            rdfs:range :Analyst ;
                            rdfs:comment "Links an artefact to the analyst who collected, created, or documented it." ;
                            rdfs:label "has artefact creation analyst" .


###  https://haulaah.github.io/CIRPO#hasChatAnalyst
:hasChatAnalyst rdf:type owl:ObjectProperty ;
                owl:inverseOf :generatedChat ;
                rdfs:domain :ResponderCommunication ;
                rdfs:range :Analyst ;
                rdfs:comment "Links a communication to the analyst who made the chat." ;
                rdfs:label "has chat analyst" .


###  https://haulaah.github.io/CIRPO#hasCoa
:hasCoa rdf:type owl:ObjectProperty ;
        owl:inverseOf :isCoaOf ;
        rdfs:domain :Incident ;
        rdfs:range :CourseOfAction ;
        rdfs:comment "Links an incident to the response actions (COA) taken to resolve it." ;
        rdfs:label "has course of action" .


###  https://haulaah.github.io/CIRPO#hasCoaAssignedAnalyst
:hasCoaAssignedAnalyst rdf:type owl:ObjectProperty ;
                       owl:inverseOf :assignedToCoa ;
                       rdfs:domain :CourseOfAction ;
                       rdfs:range :Analyst ;
                       rdfs:comment "Links a course of action to the analyst responsible for executing it." ;
                       rdfs:label "has COA assigned analyst" .


###  https://haulaah.github.io/CIRPO#hasCoaCreationAnalyst
:hasCoaCreationAnalyst rdf:type owl:ObjectProperty ;
                       owl:inverseOf :createdCoa ;
                       rdfs:domain :CourseOfAction ;
                       rdfs:range :Analyst ;
                       rdfs:comment "Links a course of action to the analyst who created or planned it." ;
                       rdfs:label "has COA creation analyst" .


###  https://haulaah.github.io/CIRPO#hasConfirmationAnalyst
:hasConfirmationAnalyst rdf:type owl:ObjectProperty ;
                        owl:inverseOf :confirmedIncident ;
                        rdfs:domain :Incident ;
                        rdfs:range :Analyst ;
                        rdfs:comment "Links an incident to the analyst who confirmed it as a genuine security incident." ;
                        rdfs:label "has confirmation analyst" .


###  https://haulaah.github.io/CIRPO#hasEvent
:hasEvent rdf:type owl:ObjectProperty ;
          owl:inverseOf :isEventOf ;
          rdfs:domain :Incident ;
          rdfs:range :SecurityEvent ;
          rdfs:comment "Links an incident to one or more related security events that triggered or are part of it." ;
          rdfs:label "has event" .


###  https://haulaah.github.io/CIRPO#hasPertainingChat
:hasPertainingChat rdf:type owl:ObjectProperty ;
                   rdfs:domain :Incident ;
                   rdfs:range :ResponderCommunication ;
                   rdfs:comment "Inverse of pertainsToIncident. Links an incident to a chat about it." ;
                   rdfs:label "has pertaining chat" .


###  https://haulaah.github.io/CIRPO#isAnalysisOf
:isAnalysisOf rdf:type owl:ObjectProperty ;
              rdfs:domain :SecurityAnalysis ;
              rdfs:range :Incident ;
              rdfs:comment "Inverse of hasAnalysis. Links an analysis back to its incident." ;
              rdfs:label "is analysis of" .


###  https://haulaah.github.io/CIRPO#isArtefactOf
:isArtefactOf rdf:type owl:ObjectProperty ;
              rdfs:domain :Artefact ;
              rdfs:range :Incident ;
              rdfs:comment "Inverse of hasArtefact. Links an artefact back to its incident." ;
              rdfs:label "is artefact of" .


###  https://haulaah.github.io/CIRPO#isCoaOf
:isCoaOf rdf:type owl:ObjectProperty ;
         rdfs:domain :CourseOfAction ;
         rdfs:range :Incident ;
         rdfs:comment "Inverse of hasCoa. Links a course of action back to its incident." ;
         rdfs:label "is COA of" .


###  https://haulaah.github.io/CIRPO#isEventOf
:isEventOf rdf:type owl:ObjectProperty ;
           rdfs:domain :SecurityEvent ;
           rdfs:range :Incident ;
           rdfs:comment "Inverse of hasEvent. Links a security event back to its parent incident." ;
           rdfs:label "is event of" .


###  https://haulaah.github.io/CIRPO#performed
:performed rdf:type owl:ObjectProperty ;
           rdfs:domain :Analyst ;
           rdfs:range [ rdf:type owl:Class ;
                        owl:unionOf ( :SecurityAnalysis
                                      :SecurityEvent
                                    )
                      ] ;
           rdfs:comment "Inverse of prov:wasPerformedBy. Indicates an analyst performed an activity." ;
           rdfs:label "performed" .


###  https://haulaah.github.io/CIRPO#performedAnalysis
:performedAnalysis rdf:type owl:ObjectProperty ;
                   rdfs:domain :Analyst ;
                   rdfs:range :SecurityAnalysis ;
                   rdfs:comment "Inverse of hasAnalysisPerformingAnalyst. Links an analyst to the analyses they performed." ;
                   rdfs:label "performed analysis" .


###  https://haulaah.github.io/CIRPO#pertainsToIncident
:pertainsToIncident rdf:type owl:ObjectProperty ;
                    owl:inverseOf :hasPertainingChat ;
                    rdfs:domain :ResponderCommunication ;
                    rdfs:range :Incident ;
                    rdfs:comment "Links a chat to the incident it discusses." ;
                    rdfs:label "pertains to incident" .


###  https://haulaah.github.io/CIRPO#wasUsedBy
:wasUsedBy rdf:type owl:ObjectProperty ;
           rdfs:domain :SecurityAnalysis ;
           rdfs:range :CourseOfAction ;
           rdfs:comment "Inverse of prov:used. Indicates an analysis was used by a course of action." ;
           rdfs:label "was used by" .


#################################################################
#    Data properties
#################################################################

###  https://haulaah.github.io/CIRPO#hasAnalystEmail
:hasAnalystEmail rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Analyst ;
                 rdfs:range xsd:string ;
                 rdfs:comment "Email address of the analyst." ;
                 rdfs:label "has analyst email" .


###  https://haulaah.github.io/CIRPO#hasAnalystId
:hasAnalystId rdf:type owl:DatatypeProperty ;
              rdfs:domain :Analyst ;
              rdfs:range xsd:string ;
              rdfs:comment "Unique identifier for an analyst." ;
              rdfs:label "has analyst ID" .


###  https://haulaah.github.io/CIRPO#hasArtefactCreationDateTime
:hasArtefactCreationDateTime rdf:type owl:DatatypeProperty ;
                             rdfs:domain :Artefact ;
                             rdfs:range xsd:dateTime ;
                             rdfs:comment "Timestamp when the artefact was collected." ;
                             rdfs:label "has artefact creation datetime" .


###  https://haulaah.github.io/CIRPO#hasArtefactDataValue
:hasArtefactDataValue rdf:type owl:DatatypeProperty ;
                      rdfs:domain :Artefact ;
                      rdfs:range xsd:string ;
                      rdfs:comment "The actual content or value of the artefact ." ;
                      rdfs:label "has artefact data value" .


###  https://haulaah.github.io/CIRPO#hasArtefactType
:hasArtefactType rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Artefact ;
                 rdfs:range xsd:string ;
                 rdfs:comment "Classification of the artefact." ;
                 rdfs:label "has artefact type" .


###  https://haulaah.github.io/CIRPO#hasChatMessage
:hasChatMessage rdf:type owl:DatatypeProperty ;
                rdfs:domain :ResponderCommunication ;
                rdfs:range xsd:string ;
                rdfs:comment "The content or body of the communication message." ;
                rdfs:label "has chat message" .


###  https://haulaah.github.io/CIRPO#hasChatMessageDateTime
:hasChatMessageDateTime rdf:type owl:DatatypeProperty ;
                        rdfs:domain :ResponderCommunication ;
                        rdfs:range xsd:dateTime ;
                        rdfs:comment "Timestamp when the communication occurred." ;
                        rdfs:label "has chat message datetime" .


###  https://haulaah.github.io/CIRPO#hasChatSentiment
:hasChatSentiment rdf:type owl:DatatypeProperty ;
                  rdfs:domain :ResponderCommunication ;
                  rdfs:range xsd:string ;
                  rdfs:comment "The sentiment or tone of the communication." ;
                  rdfs:label "has chat sentiment" .


###  https://haulaah.github.io/CIRPO#hasCoaCreationDateTime
:hasCoaCreationDateTime rdf:type owl:DatatypeProperty ;
                        rdfs:domain :CourseOfAction ;
                        rdfs:range xsd:dateTime ;
                        rdfs:comment "Timestamp when the course of action was created." ;
                        rdfs:label "has COA creation datetime" .


###  https://haulaah.github.io/CIRPO#hasCoaDescription
:hasCoaDescription rdf:type owl:DatatypeProperty ;
                   rdfs:domain :CourseOfAction ;
                   rdfs:range xsd:string ;
                   rdfs:comment "Narrative description of the course of action and its intended effect." ;
                   rdfs:label "has COA description" .


###  https://haulaah.github.io/CIRPO#hasCoaEndDateTime
:hasCoaEndDateTime rdf:type owl:DatatypeProperty ;
                   rdfs:domain :CourseOfAction ;
                   rdfs:range xsd:dateTime ;
                   rdfs:comment "Timestamp when execution of the course of action was completed." ;
                   rdfs:label "has COA end datetime" .


###  https://haulaah.github.io/CIRPO#hasCoaStartDateTime
:hasCoaStartDateTime rdf:type owl:DatatypeProperty ;
                     rdfs:domain :CourseOfAction ;
                     rdfs:range xsd:dateTime ;
                     rdfs:comment "Timestamp when execution of the course of action was initiated." ;
                     rdfs:label "has COA start datetime" .


###  https://haulaah.github.io/CIRPO#hasCoaStatus
:hasCoaStatus rdf:type owl:DatatypeProperty ;
              rdfs:domain :CourseOfAction ;
              rdfs:range xsd:string ;
              rdfs:comment "Current execution status of the course of action." ;
              rdfs:label "has COA status" .


###  https://haulaah.github.io/CIRPO#hasConfirmationDateTime
:hasConfirmationDateTime rdf:type owl:DatatypeProperty ;
                         rdfs:domain :Incident ;
                         rdfs:range xsd:dateTime ;
                         rdfs:comment "Timestamp when the incident was confirmed" ;
                         rdfs:label "has confirmation datetime" .


###  https://haulaah.github.io/CIRPO#hasEndDateTime
:hasEndDateTime rdf:type owl:DatatypeProperty ;
                rdfs:domain :Incident ;
                rdfs:range xsd:dateTime ;
                rdfs:comment "Timestamp when incident response was formally concluded." ;
                rdfs:label "has end datetime" .


###  https://haulaah.github.io/CIRPO#hasEventActivity
:hasEventActivity rdf:type owl:DatatypeProperty ;
                  rdfs:domain :SecurityEventAction ;
                  rdfs:range xsd:string ;
                  rdfs:comment "Description of the specific activity within a security event." ;
                  rdfs:label "has event activity" .


###  https://haulaah.github.io/CIRPO#hasEventActivityDateTime
:hasEventActivityDateTime rdf:type owl:DatatypeProperty ;
                          rdfs:domain :SecurityEventAction ;
                          rdfs:range xsd:dateTime ;
                          rdfs:comment "Timestamp when the specific security event activity occurred." ;
                          rdfs:label "has event activity datetime" .


###  https://haulaah.github.io/CIRPO#hasEventDescription
:hasEventDescription rdf:type owl:DatatypeProperty ;
                     rdfs:domain :SecurityEventAction ;
                     rdfs:range xsd:string ;
                     rdfs:comment "Detailed description of the security event." ;
                     rdfs:label "has event description" .


###  https://haulaah.github.io/CIRPO#hasEventId
:hasEventId rdf:type owl:DatatypeProperty ;
            rdfs:domain :SecurityEvent ;
            rdfs:range xsd:string ;
            rdfs:comment "Unique identifier for a security event." ;
            rdfs:label "has event ID" .


###  https://haulaah.github.io/CIRPO#hasEventTitle
:hasEventTitle rdf:type owl:DatatypeProperty ;
               rdfs:domain :SecurityEvent ;
               rdfs:range xsd:string ;
               rdfs:comment "Short description or name for the security event." ;
               rdfs:label "has event title" .


###  https://haulaah.github.io/CIRPO#hasIncidentCreationDateTime
:hasIncidentCreationDateTime rdf:type owl:DatatypeProperty ;
                             rdfs:domain :Incident ;
                             rdfs:range xsd:dateTime ;
                             rdfs:comment "Timestamp when the incident record was first created." ;
                             rdfs:label "has incident creation datetime" .


###  https://haulaah.github.io/CIRPO#hasIncidentDescription
:hasIncidentDescription rdf:type owl:DatatypeProperty ;
                        rdfs:domain :Incident ;
                        rdfs:range xsd:string ;
                        rdfs:comment "Detailed description of an incident. " ;
                        rdfs:label "has incident description" .


###  https://haulaah.github.io/CIRPO#hasIncidentId
:hasIncidentId rdf:type owl:DatatypeProperty ;
               rdfs:domain :Incident ;
               rdfs:range xsd:string ;
               rdfs:comment "Unique identifier for an incident." ;
               rdfs:label "has incident ID" .


###  https://haulaah.github.io/CIRPO#hasIncidentName
:hasIncidentName rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Incident ;
                 rdfs:range xsd:string ;
                 rdfs:comment "Title of the incident." ;
                 rdfs:label "has incident name" .


###  https://haulaah.github.io/CIRPO#hasIncidentSeverity
:hasIncidentSeverity rdf:type owl:DatatypeProperty ;
                     rdfs:domain :Incident ;
                     rdfs:range xsd:string ;
                     rdfs:comment "The severity level of an incident (Low, Medium, High, Critical)." ;
                     rdfs:label "has incident severity" .


###  https://haulaah.github.io/CIRPO#hasIncidentStage
:hasIncidentStage rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Incident ;
                  rdfs:range xsd:string ;
                  rdfs:comment "Current stage in the incident lifecycle." ;
                  rdfs:label "has incident stage" .


###  https://haulaah.github.io/CIRPO#hasIncidentStatus
:hasIncidentStatus rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Incident ;
                   rdfs:range xsd:string ;
                   rdfs:comment "Current status of the incident." ;
                   rdfs:label "has incident status" .


###  https://haulaah.github.io/CIRPO#hasSecurityAnalysisDataValue
:hasSecurityAnalysisDataValue rdf:type owl:DatatypeProperty ;
                              rdfs:domain :SecurityAnalysis ;
                              rdfs:range xsd:string ;
                              rdfs:comment "The data value to be analysed." ;
                              rdfs:label "has security analysis data value" .


###  https://haulaah.github.io/CIRPO#hasSecurityAnalysisDateTime
:hasSecurityAnalysisDateTime rdf:type owl:DatatypeProperty ;
                             rdfs:domain :SecurityAnalysis ;
                             rdfs:range xsd:dateTime ;
                             rdfs:comment "Timestamp when the security analysis was performed." ;
                             rdfs:label "has security analysis datetime" .


###  https://haulaah.github.io/CIRPO#hasSecurityAnalysisDetails
:hasSecurityAnalysisDetails rdf:type owl:DatatypeProperty ;
                            rdfs:domain :SecurityAnalysis ;
                            rdfs:range xsd:string ;
                            rdfs:comment "Detailed findings from the analysis." ;
                            rdfs:label "has security analysis details" .


###  https://haulaah.github.io/CIRPO#hasSecurityAnalysisType
:hasSecurityAnalysisType rdf:type owl:DatatypeProperty ;
                         rdfs:domain :SecurityAnalysis ;
                         rdfs:range xsd:string ;
                         rdfs:comment "The type of analysis performed." ;
                         rdfs:label "has security analysis data type" .


#################################################################
#    Classes
#################################################################

###  https://haulaah.github.io/CIRPO#Analysis
:Analysis rdf:type owl:Class ;
          rdfs:subClassOf :CourseOfAction ;
          rdfs:comment "Activities to assess security incidents." ;
          rdfs:label "Analysis" .


###  https://haulaah.github.io/CIRPO#Analyst
:Analyst rdf:type owl:Class ;
         rdfs:comment "A security analyst who investigates and responds to incidents." ;
         rdfs:label "Analyst" .


###  https://haulaah.github.io/CIRPO#Artefact
:Artefact rdf:type owl:Class ;
          rdfs:comment "An observable piece of evidence collected from an incident." ;
          rdfs:label "Artefact" .


###  https://haulaah.github.io/CIRPO#Containment
:Containment rdf:type owl:Class ;
             rdfs:subClassOf :CourseOfAction ;
             rdfs:comment "Actions to limit the spread and impact of an incident." ;
             rdfs:label "Containment" .


###  https://haulaah.github.io/CIRPO#CourseOfAction
:CourseOfAction rdf:type owl:Class ;
                rdfs:comment "A planned response action to a security incident." ;
                rdfs:label "Course of Action" .


###  https://haulaah.github.io/CIRPO#Detection
:Detection rdf:type owl:Class ;
           rdfs:subClassOf :CourseOfAction ;
           rdfs:comment "Activities in detection of security incidents." ;
           rdfs:label "Detection" .


###  https://haulaah.github.io/CIRPO#Eradication
:Eradication rdf:type owl:Class ;
             rdfs:subClassOf :CourseOfAction ;
             rdfs:comment "Actions to remove threats and root causes from affected systems." ;
             rdfs:label "Eradication" .


###  https://haulaah.github.io/CIRPO#Incident
:Incident rdf:type owl:Class ;
          rdfs:comment "A security incident that requires investigation and response." ;
          rdfs:label "Incident" .


###  https://haulaah.github.io/CIRPO#PostIncidentActivity
:PostIncidentActivity rdf:type owl:Class ;
                      rdfs:subClassOf :CourseOfAction ;
                      rdfs:comment "Activities conducted after incident resolution (e.g., lessons learned, reporting)." ;
                      rdfs:label "Post-Incident Activity" .


###  https://haulaah.github.io/CIRPO#Recovery
:Recovery rdf:type owl:Class ;
          rdfs:subClassOf :CourseOfAction ;
          rdfs:comment "Actions to restore systems to normal operation." ;
          rdfs:label "Recovery" .


###  https://haulaah.github.io/CIRPO#ResponderCommunication
:ResponderCommunication rdf:type owl:Class ;
                        rdfs:comment "Communication between incident responders during an investigation." ;
                        rdfs:label "Responder Communication" .


###  https://haulaah.github.io/CIRPO#SecurityAnalysis
:SecurityAnalysis rdf:type owl:Class ;
                  rdfs:comment "The analysis of observables from security events leading to an incident" ;
                  rdfs:label "Security Analysis" .


###  https://haulaah.github.io/CIRPO#SecurityEvent
:SecurityEvent rdf:type owl:Class ;
               rdfs:comment "An observable occurrence relevant to system security (e.g., alert from SIEM, IDS)." ;
               rdfs:label "Security Event" .


###  https://haulaah.github.io/CIRPO#SecurityEventAction
:SecurityEventAction rdf:type owl:Class ;
                     rdfs:subClassOf :SecurityEvent ;
                     rdfs:comment "A specific action taken or observed within a security event." ;
                     rdfs:label "Security Event Action" .


###  https://haulaah.github.io/CIRPO#Tier1
:Tier1 rdf:type owl:Class ;
       rdfs:subClassOf :Analyst ;
